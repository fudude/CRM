<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>MyServlet01</servlet-name>
        <servlet-class>com.dude.servlets.MyServlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet01</servlet-name>
        <url-pattern>/myServlet01.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MyServlet02</servlet-name>
        <servlet-class>com.dude.servlets.MyServlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet02</servlet-name>
        <url-pattern>/myServlet02.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.dude.servlets.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/student/save.do</url-pattern>
        <url-pattern>/student/update.do</url-pattern>
        <url-pattern>/student/delete.do</url-pattern>
        <url-pattern>/student/select.do</url-pattern>
    </servlet-mapping>
</web-app>